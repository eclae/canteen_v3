{% extends 'SICantineBundle::layout.html.twig' %}

{% block stylesheets %}
<link href="http://code.jquery.com/ui/1.10.2/themes/smoothness/jquery-ui.min.css" rel="stylesheet" media="screen" />
<link href="{{ asset('css/jquery-plugin/jquery-ui-timepicker-addon.css') }}" rel="stylesheet" media="screen" />
 {% block CSS_addon %}
 {% endblock %}
{% endblock stylesheets %}
{% block javascripts %}
<script src="{{ asset('js/jquery-plugin/jquery-ui-timepicker-addon.js') }}"></script>
<script src="{{ asset('js/jquery-plugin/jquery-ui-sliderAccess.js') }}"></script>
 {% block JS_addon %}
 {% endblock %}
{% endblock javascripts %}

{% block content -%}
<h1>{{ 'cantine.title.child.create'|trans }}</h1>

<form action="{{ path('admin_child_create') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
    <p>
      <input type="submit" class="btn btn-primary" value="{{ 'cantine.action.new.child'|trans }}" />
    </p>
  </form>
  <script>
    $(document).ready(function() {
      $("#si_cantinebundle_childtype_users").select2();
    });
    </script>

{% endblock %}
